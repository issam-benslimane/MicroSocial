{
  "compilerOptions": {
    "target": "ES6",
    "outDir": "./build/",
    "module": "CommonJS",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": ["./src/**/*", "./prisma/*.ts"],
  "exclude": ["node_modules"]
}
